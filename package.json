    "@types/react-dom": "16.8.4",
    "apollo": "^2.8.3",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-http": "^1.5.14",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "bootstrap": "^4.3.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.4",
    "react-app-polyfill": "^0.2.2",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "reactstrap": "^8.0.0",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.4.3",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "dev": "node scripts/start.js",
    "dev-lint": "yarn lint:ts && yarn lint:css && node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "lint:ts": "tslint -t codeFrame --project tsconfig.json ./{src,@types,config}/**/*.{ts,tsx} && tsc --project tsconfig.json",
    "lint:css": "stylelint ./src/**/*.{ts,tsx}",
    "generate-apollo-types": "yarn remove-apollo-types && yarn download-schema && yarn get-apollo-types",
    "remove-apollo-types": "node ./tools/removeApolloTypes",
    "download-schema": "apollo service:download graphql.schema.json --endpoint=http://localhost:4000",
    "get-apollo-types": "apollo client:codegen apolloTypes --no-addTypename --target typescript --localSchemaFile graphql.schema.json --globalTypesFile ./src/common/graphql/ApolloTypes/globalTypes.ts --includes **/*.graphql"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "/home/moonmoon/WebstormProjects/krasnovidovo-admin/node_modules/jest-watch-typeahead/filename.js",
      "/home/moonmoon/WebstormProjects/krasnovidovo-admin/node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "@babel/env",
      "@babel/react",
      "@babel/typescript"
    ],
    "plugins": [
      "babel-plugin-styled-components"
    ]
  },
  "lint-staged": {
    "*.js": [
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint -t codeFrame --project tsconfig.json --fix",
      "git add"
    ],
    "./src/**/*.{tsx,ts}": [
      "stylelint"
    ],
    "*.{js,ts,tsx,json,graphql,md,*rc}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.123",
    "@types/react-router-dom": "^4.3.2",
    "@types/reactstrap": "^8.0.1",
    "@types/styled-components": "^4.1.14",
    "babel-plugin-styled-components": "^1.10.0",
    "lint-staged": "^8.1.5",
    "styled-components": "^4.2.0",
    "stylelint": "^10.0.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.6.0",
    "tslint": "^5.15.0"
  }
}
